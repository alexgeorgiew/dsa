#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <string>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    string first,second;
    cin>>first;
    cin>>second;
   unsigned int i=0;
    int curcount=0;
    int maxcount=-1;
   
    while(i<first.size() && i<second.size())
    {
        if(first.at(i)==second.at(i))
        {
            curcount++;
        }
        else
        {
            if(maxcount<curcount)
            {
                maxcount=curcount;
                
            }
            curcount=0;
        }
        i++;
    }
   cout<<maxcount;
    return 0;
}
